<Window x:Class="TeleFocusUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="TeleFocus" Height="234" Width="413">
    <DockPanel>
        <StackPanel Height="200" Width="100" DockPanel.Dock="Left">
            <Button x:Name="FwdButton" Content="FWD" Width="100" Height="100" IsEnabled="False"/>
            <Separator/>
            <Button x:Name="BwdButton" Content="BWD" Width="100" Height="100" IsEnabled="False"/>
        </StackPanel>
        <ListBox x:Name="LogListBox" Width="300" Height="200" />
    </DockPanel>
</Window>
